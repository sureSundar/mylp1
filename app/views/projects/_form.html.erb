<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :user %>
    <%= f.input :logo %>
    <%= f.association :hpuser %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
